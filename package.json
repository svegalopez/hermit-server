{
  "name": "typescript-jest-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "tsc && dotenv -e ./src/hermit/prisma/.env.local -- node dist/index.js",
    "start:watch": "nodemon -e ts --exec \"npm start\"",
    "migrate": "dotenv -e ./src/hermit/prisma/.env.local -- prisma migrate dev --schema ./src/hermit/prisma/schema.prisma",
    "studio": "dotenv -e ./src/hermit/prisma/.env.local -- prisma studio --schema ./src/hermit/prisma/schema.prisma",
    "reset": "dotenv -e ./src/hermit/prisma/.env.local -- prisma migrate reset --schema ./src/hermit/prisma/schema.prisma",
    "test": "jest --verbose"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/supertest": "^2.0.12",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.22",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "jest": "^29.0.3",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "prisma": "^4.3.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.2"
  },
  "prisma": {
    "seed": "node dist/hermit/prisma/seed/index.js"
  }
}
